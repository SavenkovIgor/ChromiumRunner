{
    "browser_path": "C:/Program Files/Google/Chrome/Application/chrome.exe",
    "args": [
        {
            "description": "Path to the user data dir",
            "enabled": false,
            "arg": "user-data-dir",
            "type": "string",
            "value": "/tmp/chromium-profile"
        },
        {
            "description": "Enables logging for all process types.",
            "enabled": true,
            "arg": "enable-logging",
            "type": "flag"
        },
        {
            "description": "Sets the logging verbosity level (0-3).",
            "enabled": true,
            "arg": "v",
            "type": "number",
            "value": 1
        },
        {
            "description": "Sets the initial window size. Format: WIDTH,HEIGHT",
            "enabled": false,
            "arg": "window-size",
            "type": "string",
            "value": "1920,1080"
        },
        {
            "description": "Specifies which modules to log.",
            "enabled": true,
            "arg": "vmodule",
            "type": "list",
            "value": [
                {"enabled": true, "value": "net=2"},
                {"enabled": false, "value": "gpu=1"},
                {"enabled": true, "value": "renderer=3"}
            ]
        }
    ]
}
